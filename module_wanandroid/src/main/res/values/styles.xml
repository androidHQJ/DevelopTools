<resources>

    <!-- Base application theme. -->
    <style name="WanBaseAppTheme" parent="@style/Theme.AppCompat.DayNight">
        <item name="colorPrimary">@color/common_colorPrimary</item>
        <item name="colorPrimaryDark">@color/common_colorPrimaryDark</item>
        <item name="colorAccent">@color/common_colorAccent</item>
        <!--关闭启动窗口-->
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="AppTheme" parent="WanBaseAppTheme.NoActionBar"/>

    <style name="WanBaseAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
