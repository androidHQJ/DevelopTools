<?xml version="1.0" encoding="utf-8"?>
<com.wxq.commonlibrary.dispatch.WaterFallLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".TwoFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_one"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="match_parent">

    </androidx.recyclerview.widget.RecyclerView>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_two"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="match_parent">

    </androidx.recyclerview.widget.RecyclerView>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_three"
        android:layout_weight="1"
        android:layout_width="0dp"
        android:layout_height="match_parent">

    </androidx.recyclerview.widget.RecyclerView>




</com.wxq.commonlibrary.dispatch.WaterFallLinearLayout>