<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="TagFlowLayout_left_margin" format="dimension"/> <!--左间距-->
        <attr name="TagFlowLayout_top_margin" format="dimension"/> <!--右间距-->
        <attr name="TagFlowLayout_right_margin" format="dimension"/> <!--上间距-->
        <attr name="TagFlowLayout_bottom_margin" format="dimension"/> <!--下间距-->


        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
</resources>